<p style="color: green"><%= notice %></p>

<h1>Members</h1>
<%= link_to "Add New member", new_member_path %></br>

<div id="members">
  <% @members.each do |member| %>
    <p>
    <strong>Name:</strong>
    <%= member.first_name %> <%= member.last_name %>
    </p>
    <p>
      <%= link_to "Show this member", member %>
    </p>
  <% end %>
</div>

