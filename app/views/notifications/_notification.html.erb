<div id="<%= dom_id notification %>">
  <p>
    <strong>Description:</strong>
    <%= notification.description %>
  </p>

  <p>
    <strong>Send time:</strong>
    <%= notification.send_time %>
  </p>

  <p>
    <strong>Send date:</strong>
    <%= notification.send_date %>
  </p>

  <p>
    <strong>Is sent:</strong>
    <%= notification.is_sent %>
  </p>

</div>
